{
  "name": "@erickmerchant/time-tracking-app",
  "productName": "Time Tracker",
  "private": true,
  "version": "1.0.8",
  "description": "a simple app to track time",
  "license": "MIT",
  "repository": "https://github.com/erickmerchant/time-tracking-app",
  "author": "Erick Merchant <erickmerchant@gmail.com>",
  "scripts": {
    "test": "standard",
    "watch": "electron . & assets ./build -w --electron",
    "preview": "electron .",
    "build": "npm test && assets ./build --electron && npm run package-win32 && npm run package-darwin && npm run package-linux",
    "package-win32": "electron-packager . --out=dist --asar --overwrite --arch=x64 --platform=win32",
    "package-darwin": "electron-packager . --out=dist --asar --overwrite --arch=x64 --platform=darwin",
    "package-linux": "electron-packager . --out=dist --asar --overwrite --arch=x64 --platform=linux"
  },
  "dependencies": {
    "@erickmerchant/assets": "^10.1.2",
    "@erickmerchant/framework": "^13.0.1",
    "bel": "^5.1.5",
    "devtron": "^1.1.0",
    "electron": "^1.8.1",
    "electron-debug": "^1.4.0",
    "electron-packager": "^10.1.0",
    "electron-store": "^1.3.0",
    "feather-icons": "^4.0.1",
    "nanomorph": "^5.1.3",
    "standard": "^10.0.3",
    "uuid": "^3.1.0"
  }
}
